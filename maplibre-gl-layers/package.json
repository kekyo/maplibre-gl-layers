{
  "name": "maplibre-gl-layers",
  "version": "0.0.1",
  "keywords": [
    "maplibre",
    "webgl",
    "layer",
    "many-sprites",
    "automatic-interpolation",
    "hud",
    "interaction-events",
    "grouping"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "images/maplibre-gl-layers-120.png",
    "images/demo1.png",
    "images/demo2.png"
  ],
  "scripts": {
    "build": "node ./scripts/build-wasm.mjs && vite build",
    "dev": "npm run build",
    "test": "npm run build && vitest run",
    "test:e2e": "npm run build && playwright test",
    "pack": "npm run build && screw-up pack --pack-destination ../artifacts"
  },
  "dependencies": {
    "maplibre-gl": ">=5.0.0"
  },
  "devDependencies": {
    "@playwright/test": ">=1.56.0",
    "@types/node": ">=24.9.0",
    "prettier-max": ">=1.11.0",
    "screw-up": ">=1.13.0",
    "typescript": ">=5.0.0",
    "vite": ">=5.0.0",
    "vite-plugin-dts": ">=4.5.0",
    "vitest": ">=1.6.0"
  }
}
